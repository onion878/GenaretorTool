{
  "name": "GenaretorTool",
  "version": "1.0.0",
  "description": "A genaretor Electron application",
  "main": "main.js",
  "scripts": {
    "install": "node copy.js",
    "start": "electron .",
    "postinstall": "install-app-deps",
    "package-win": "electron-packager . --overwrite --asar --platform=win32 --arch=x64 --electron-version 4.0.0 --icon=static/images/icon.ico --out=dist --version-string.CompanyName=OnionTool --version-string.FileDescription=GenaretorTool --version-string.ProductName=\"GenaretorTool\" && node renderer.js",
    "package-linux": "electron-packager . --overwrite --asar --platform=linux --arch=x64 --electron-version 4.0.0 --icon=static/images/icon.ico --out=dist --version-string.CompanyName=OnionTool --version-string.FileDescription=GenaretorTool --version-string.ProductName=\"GenaretorTool\""
  },
  "keywords": [
    "Electron",
    "code",
    "node",
    "extjs"
  ],
  "author": "Onion",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^4.0.0",
    "electron-builder": "^20.38.2"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "archiver": "^2.1.1",
    "axios": "^0.18.0",
    "decompress": "^4.2.0",
    "del": "^3.0.0",
    "lowdb": "^1.0.0",
    "monaco-editor": "^0.15.6",
    "node-pty": "^0.7.8",
    "open": "0.0.5",
    "require-uncached": "^1.0.3",
    "shelljs": "^0.8.2",
    "swig": "^1.4.2",
    "xterm": "^3.8.0"
  }
}
