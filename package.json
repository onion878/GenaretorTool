{
  "name": "GenerateTool",
  "version": "1.5.9",
  "description": "A Generate Code Tool",
  "main": "main.js",
  "scripts": {
    "install": "node copy.js",
    "start": "electron .",
    "dist": "electron-builder --x64",
    "postinstall": "install-app-deps"
  },
  "keywords": [
    "Electron",
    "code",
    "node",
    "extjs"
  ],
  "author": "Onion",
  "license": "CC0-1.0",
  "url": "http://onion.zhouhongxing.cn",
  "build": {
    "appId": "onion.app.GenaretorTool",
    "asar": true,
    "mac": {
      "icon": "static/images/code.png",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "static/images/code.png",
      "target": [
        "nsis",
        "zip"
      ]
    }
  },
  "devDependencies": {
    "electron": "^4.2.6",
    "electron-builder": "^20.44.4"
  },
  "dependencies": {
    "ansicolor": "^1.1.92",
    "app-root-path": "^2.2.1",
    "archiver": "^3.0.0",
    "decompress": "^4.2.0",
    "del": "^5.0.0",
    "log4js": "^4.4.0",
    "lowdb": "^1.0.0",
    "marked": "^0.7.0",
    "monaco-editor": "^0.17.1",
    "node-pty": "^0.8.1",
    "open": "^6.4.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "require-uncached": "^2.0.0",
    "shelljs": "^0.8.3",
    "swig": "^1.4.2",
    "xterm": "^3.14.4"
  }
}
