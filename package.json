{
  "name": "GenerateTool",
  "version": "1.7.4",
  "description": "A Generate Code Tool",
  "main": "main.js",
  "scripts": {
    "install": "node copy.js",
    "start": "electron .",
    "dist": "electron-builder --mac --arm64",
    "postinstall": "node prebuild.js && install-app-deps"
  },
  "keywords": [
    "Electron",
    "code",
    "node",
    "extjs"
  ],
  "author": {
    "name": "Onion",
    "email": "a2214839296a@gmail.com"
  },
  "homepage": "https://github.com/onion878/GenerateTool",
  "license": "CC0-1.0",
  "url": "http://onion.zhouhongxing.cn",
  "build": {
    "appId": "onion.app.GenaretorTool",
    "asar": true,
    "mac": {
      "icon": "static/images/code.png",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "static/images/code.png",
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "snap",
        "deb"
      ],
      "icon": "static/images/code.icns"
    }
  },
  "devDependencies": {
    "electron": "^11.3.0",
    "electron-builder": "^22.10.5"
  },
  "dependencies": {
    "ansicolor": "^1.1.92",
    "app-root-path": "^3.0.0",
    "archiver": "^5.2.0",
    "decompress": "^4.2.0",
    "del": "^6.0.0",
    "log4js": "^6.1.0",
    "lowdb": "^1.0.0",
    "marked": "^2.0.1",
    "monaco-editor": "^0.22.3",
    "node-pty": "^0.10.0",
    "open": "^7.4.2",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "require-uncached": "^2.0.0",
    "sequelize": "^6.5.0",
    "shelljs": "^0.8.3",
    "sqlite3": "^5.0.2",
    "swig": "^1.4.2",
    "windows-titlebar-color": "^1.2.0",
    "xterm": "^4.10.0",
    "xterm-addon-fit": "^0.5.0"
  }
}
