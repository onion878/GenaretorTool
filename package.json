{
  "name": "GenerateTool",
  "version": "1.0.0",
  "description": "A Generate Code Tool",
  "main": "main.js",
  "scripts": {
    "install": "node copy.js",
    "start": "electron .",
    "dist": "electron-builder --win --x64",
    "postinstall": "install-app-deps"
  },
  "keywords": [
    "Electron",
    "code",
    "node",
    "extjs"
  ],
  "author": "Onion",
  "license": "CC0-1.0",
  "build": {
    "appId": "onion.app.GenaretorTool",
    "asar": true,
    "mac": {
      "icon": "static/images/icon.ico",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "static/images/icon.ico",
      "target": [
        "nsis",
        "zip"
      ]
    }
  },
  "devDependencies": {
    "electron": "^4.0.4",
    "electron-builder": "^20.38.5"
  },
  "dependencies": {
    "app-root-path": "^2.1.0",
    "archiver": "^3.0.0",
    "axios": "^0.18.0",
    "decompress": "^4.2.0",
    "del": "^3.0.0",
    "lowdb": "^1.0.0",
    "monaco-editor": "^0.15.6",
    "node-pty": "^0.8.1",
    "open": "0.0.5",
    "require-uncached": "^2.0.0",
    "shelljs": "^0.8.3",
    "swig": "^1.4.2",
    "xterm": "^3.11.0"
  }
}
